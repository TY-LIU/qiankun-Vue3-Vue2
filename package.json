{
  "name": "qiankun-vue3-vue2",
  "version": "1.0.0",
  "description": "用`qiankun`搭建的`vue`微前端项目",
  "author": "lty <1536237744@qq.com>",
  "main": "index.js",
  "scripts": {
    "install:doc": "cd doc && yarn install",
    "install:main": "cd main && yarn install",
    "install-all": "npm-run-all install:*",
    "start:doc": "cd doc && npm run serve",
    "start:main": "cd main && npm run serve",
    "start-all": "npm-run-all --parallel start:*",
    "serve-all": "npm-run-all --parallel start:*",
    "build:doc": "cd doc && npm run build",
    "build:main": "cd main && npm run build",
    "build-all": "npm-run-all --parallel build:*"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "repository": "https://github.com/TY-LIU/qiankun-Vue3-Vue2.git",
  "license": "MIT"
}
